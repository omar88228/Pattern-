package builder;

import entity.Address;
import entity.IEstate;
import entity.Immobile;
import enumeration.Type;

public class BuilderImmobile  implements IBuilderImmobile {
	Immobile immobile ;
	IEstate estate;
	public BuilderImmobile()  {
		immobile = new Immobile();
	}
	
	@Override
	public IBuilderImmobile surface(int surface) {
		immobile.setSurface(surface);
		return this;
	}
	@Override
	public IBuilderImmobile localNumber(int localNumber) {
		immobile.setLocalNumber(localNumber);
		return this;
	}
	@Override
	public IBuilderImmobile floor(int floor) {
		immobile.setFloor(floor);
		
		return this;
	}
	@Override
	public IBuilderImmobile address(Address address) {
		immobile.setAddress(address);
		return this;
	}
	@Override
	public IBuilderImmobile type(String type) {
		immobile.setType(Type.valueOf(type));
		return this;
	}
	@Override
	public Immobile build() {
		
		return immobile;
	}



}

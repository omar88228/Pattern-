package Builder;

import entity.Appartment;
import entity.Immobile;
import entity.Villa;

public class AppartmentBuilder extends AbstractBuilder<AppartmentBuilder> {
	
	private Appartment appartment;
	

	public AppartmentBuilder() {
		super();
		appartment= new Appartment();
	}

	public AppartmentBuilder floor(int floor) {
		appartment.setFloor(floor);
		return this ;
	}


	@Override
	public AppartmentBuilder getThis() {
		// TODO Auto-generated method stub
		return this;
	}

	@Override
	public Appartment build() {
		// TODO Auto-generated method stub
		return appartment;
	}

}

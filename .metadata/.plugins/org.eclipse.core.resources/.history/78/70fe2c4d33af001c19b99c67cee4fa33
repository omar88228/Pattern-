package entity;

public class Appartment extends Immobile {
	private int floor ;

	public int getFloor() {
		return floor;
	}

	public void setFloor(int floor) {
		this.floor = floor;
	}
	
	@Override
	public String estateType() {
		// TODO Auto-generated method stub
		return super.estateType()+" the floor number is : "+this.floor ;
	}
	
	
}
